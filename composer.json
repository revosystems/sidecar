{
    "name": "revosystems/sidecar",
    "description": "Laravel report tool",
    "type": "library",
    "require": {
        "php": "^8.0",
        "laravel/framework": "8.x|9.x|10.x",
        "revosystems/dejavu": "^0.2"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Jordi Puigdell√≠vol",
            "email": "jordi.p@revo.works"
        }
    ],
    "autoload": {
        "psr-4": {
            "Revo\\Sidecar\\": "src"
        }
    },
    "extra": {
    	"laravel": {
